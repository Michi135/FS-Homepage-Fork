  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 shx --silent rm -rf ./production && npm run build:vue && npm run build:ts && shx cp -r ./dist-ssr/dist ./production/dist-ssr",
    "build:ts": "cross-env NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:vue": "cross-env NODE_ENV=production tsx ./src/server/build.ts",
    "start": "npm run dev",
    "dev": "npx tsx watch \"./src/server/entry.ts\"",
    "dev:detailed": "cross-env DEBUG=vite-tsconfig-paths npm run dev",
    "start:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node ./production/src/server/entryProduction.js",
    "fix": "npx eslint \"./src/**/*.{vue, ts}\" --fix",
    "lint": "npx eslint \"./src/**/*.{vue, ts}\"",
    "postinstall": "patch-package"
  },
