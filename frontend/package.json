  "scripts": {
    "build": "shx --silent rm -rf ./production && npm run build:vue && npm run build:ts && shx cp -r ./dist-ssr/dist ./production/dist-ssr",
    "build:ts": "cross-env NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:vue": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-paths-esm-loader ./src/server/build.ts",
    "start": "npm run start:devWorkaround",
    "start:devWorkaround": "npx nodemon",
    "start:dev": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register ./src/server/entry.ts ",
    "start:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node ./production/src/server/entryProduction.js",
    "patch": "ts-node ./patch.ts",
    "start:esm": "cross-env NODE_ENV=development tsnd -r ts-node/esm -r tsconfig-paths/register ./src/server/entry.ts",
    "fix": "npx eslint \"./src/**/*.{vue, ts}\" --fix",
    "lint": "npx eslint \"./src/**/*.{vue, ts}\""
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
